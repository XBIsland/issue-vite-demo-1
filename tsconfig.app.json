{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "typings/*"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "isolatedModules": true,
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "typings/*": ["./typings/*"]
    }
  }
}
